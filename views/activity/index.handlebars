<!--/.Navbar-->
<nav class="navbar navbar-expand-lg navbar-dark fixed-top scrolling-navbar">
  <div class="container">
    <a class="navbar-brand" href="/">Time is Money</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <ul class="navbar-nav ml-auto nav-flex-icons">
        <li class="nav-item dropdown">
          <a class="nav-link dropdown-toggle" id="navbarDropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><i class="fa fa-user">{{user.username}} </i> My Account</a>
          <div class="dropdown-menu dropdown-menu-right dropdown-unique" aria-labelledby="navbarDropdownMenuLink">
            <a class="dropdown-item" href="#">Settings</a>
            <a class="dropdown-item" href="/logout">Log Out</a>
          </div>
        </li>
      </ul>
    </div>
  </div>
</nav>

<section class="view intro-3 hm-gradient" style="margin-top: 10px">
    <div class="full-bg-img">
        <div class="container flex-center">
        <div class="col-md-6">
            <!--List-->
            <div class="card wow fadeInRight" data-wow-delay="0.5s">
                <div class="card-body">
                    <!--Header-->
                    <div class="text-center">
                        <h3 class="white-text"><i class="fa fa-server white-text"></i> Services You Requested</h3>
                        <hr class="hr-light">
                    </div>
                    <div class="text-justify">
                        <!--Body-->
                        {{#each services}}
                            <div class="service-preview">
                                <h4 class="service-name">
                                    {{service_name}}
                                </h4>
                                <li>
                                    Status: {{this.is_accepted}}
                                </li>
                                <li>
                                    Time: {{this.requested_datetime}}
                                </li>
                                <li>
                                    Credit: {{this.num_hours}}
                                </li>
                            </div>
                            <hr>
                        {{/each}}
                    </div>
                </div>
            </div>
        <!--/.List-->
        </div>

        <div class="col-md-6">                        <!--Form-->
            <div class="card wow fadeInRight" data-wow-delay="0.5s">
                <div class="card-body">
                    <!--Header-->
                    <div class="text-center">
                        <h3 class="white-text"><i class="fa fa-server white-text"></i> Services You Offered</h3>
                        <hr class="hr-light">
                    </div>
                    <div class="text-justify">
                        <!--Body-->
                        {{#each services}}
                            <div class="service-preview">
                                <h4 class="service-name">
                                    {{service_name}}
                                </h4>
                                <li>
                                    Status: {{this.is_accepted}}
                                </li>
                                <li>
                                    Time: {{this.requested_datetime}}
                                </li>
                                <li>
                                    Credit: {{this.num_hours}}
                                </li>
                            </div>
                            <hr>
                        {{/each}}
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>